class SingleMetavarFormatter(argparse.HelpFormatter):

    def _format_action_invocation(self, action):

        if not action.option_strings:
            metavar, = self._metavar_formatter(action, action.dest)(1)
            return metavar.upper()
        else:
            parts = []
            parts.extend(action.option_strings)
            if action.nargs != 0:
                metavar = self._metavar_formatter(action, action.dest)(1)
            return ' '.join(parts)

class Colors:
    RED = '\033[91m'
    GREEN = '\033[92m'
    YELLOW = '\033[93m'
    CYAN = '\033[96m'
    RESET = '\033[0m'
    BOLD = '\033[1m'

banner =r"""
     _     _            _                                                 _ _  _            
    (_)   (_)          | |     _                                         | | |(_)       _   
     _______ _____  ___| |__ _| |_ _____  ____ _____ _ _ _  ___   ____ __| | | _  ___ _| |_ 
    |  ___  (____ |/___)  _ (_   _|____ |/ _  (_____) | | |/ _ \ / ___) _  | || |/___|_   _)
    | |   | / ___ |___ | | | || |_/ ___ ( (_| |     | | | | |_| | |  ( (_| | || |___ | | |_ 
    |_|   |_\_____(___/|_| |_| \__)_____|\___ |      \___/ \___/|_|   \____|\_)_(___/   \__)
                                        (_____|                                             
                                                                Hashtag-Wordlist
                                                        https://github.com/Hashtag-AMIN
"""

wordlist_flags = [
    
        "subdomain",
        "directory",
        "api",
        "file",
        "word",
        "parameter",
        "header",
        "number",
        "username",
        "password",
        "technology",
        "payload",
        "webshell"
    ]

print_lock = threading.Lock()

chars = [ "|", "/", "\\", "-"]

def strip_date(filename):
    return re.sub(r'_\d{4}_\d{2}_\d{2}', '', filename)

def get_download_object(wordlist_mian_types, filter_object, download_type):
    
    download_wordlist_objs = []
    
    if download_type in ["input", "all"]:
        for wordlist_mian_type in wordlist_mian_types["wordlist"]:
            for wordlist_objs in wordlist_mian_type:
                for wordlist_obj in wordlist_mian_type[wordlist_objs]:
                    download_wordlist_objs.append(wordlist_obj)
                    
    if download_type == "custom":
        for wordlist_mian_type in wordlist_mian_types["wordlist"]:
            for wordlist_objs in wordlist_mian_type:
                if wordlist_objs in filter_object:
                    for wordlist_obj in wordlist_mian_type[wordlist_objs]:
                        download_wordlist_objs.append(wordlist_obj)            
    
    return download_wordlist_objs

def get_assetnote_link():
    url = "https://wordlists-cdn.assetnote.io/data/automated.json"
    response = requests.get(url)
    if response.status_code == 200:
        data = response.json()
        
        url_pattern = r"<a\s+href='(.*?)'>"
        parsed_data = []
        for entry in data["data"]:
            parsed_entry = {
                "Filename": entry.get("Filename"),
                "Download": re.search(url_pattern, entry.get("Download")).group(1)
            }
            parsed_data.append(parsed_entry)
            
        return parsed_data
    else:
        print(f"{Colors.RED}[!] Failed to fetch data from AssetNote - status code: {Colors.YELLOW}{response.status_code}{Colors.RESET}")
        return []

def update_dynamic_obj(wordlist_obj_for_download, assetnote_links):

    assetnote_map = {}
    for item in assetnote_links:
        base_name = strip_date(item["Filename"])
        assetnote_map[base_name] = item["Download"]

    for wordlist_obj in wordlist_obj_for_download:
        if wordlist_obj["type"].lower() == "dynamic" and wordlist_obj["provider"].lower() == "assetnote":
            obj_base_name = strip_date(wordlist_obj["fileName"])
            if obj_base_name in assetnote_map:
                wordlist_obj["fileName"] = assetnote_map[obj_base_name].split("/")[-1]
                wordlist_obj["url"] = assetnote_map[obj_base_name]

    return wordlist_obj_for_download

def download_file(url, filename, folder, dest_path, retry, timeout, retry_delay, verbose, silent):
    
    attempt = 0
    while attempt < retry:
        try:
            with requests.get(url, stream=True, timeout=timeout, allow_redirects=True, verify=False) as req:
                if req.status_code != 200:
                    if not silent:
                        print(f"\r{Colors.RED}{Colors.BOLD}[!] Download failed with status code: {Colors.YELLOW}[{req.status_code}] [{url}]{Colors.RESET}")
                    return False

                downloaded_size = 0
                with open(dest_path, 'wb') as f:
                    for chunk in req.iter_content(chunk_size=1024 * 1024):
                        if chunk:
                            f.write(chunk)
                            downloaded_size += len(chunk)
                            print(f"\r{Colors.BOLD}[+][{random.choice(chars)}] Downloading: {downloaded_size / 1024:.2f} KB of {filename}{Colors.RESET}", end='')
                    print()
                if not silent:
                    print(f"\r{Colors.CYAN}[+] Download {filename} completed with {downloaded_size} size in {folder}/{Colors.RESET}", )
                
                return True

        except (requests.exceptions.Timeout, requests.exceptions.ConnectionError) as e:
            attempt += 1
            if verbose:
                print(f"\r\nAttempt {attempt} failed: {e}. Retrying in {retry_delay * attempt} seconds...", flush=True)
            time.sleep(retry_delay * attempt)

    print(f"\r{Colors.RED}Download failed after {attempt} attempts.{Colors.RESET}")
    return False

def valid_url(url):
    parsed = urlparse(url)
    return all([parsed.scheme in ('http', 'https'), parsed.netloc])

def main():

    parser = argparse.ArgumentParser(formatter_class=SingleMetavarFormatter)
    parser.add_argument('-S', '--subdomain', help='Download subdomin wordlists',  action='store_true')
    parser.add_argument('-D', '--directory', help='Download directory wordlists',  action='store_true')
    parser.add_argument('-A', '--api', help='Download api wordlists',  action='store_true')
    parser.add_argument('-F', '--file', help='Download file wordlists',  action='store_true')
    parser.add_argument('-W', '--word', help='Download word wordlists',  action='store_true')
    parser.add_argument('-P', '--parameter', help='Download parameter wordlists',  action='store_true')
    parser.add_argument('-H', '--header', help='Download header wordlists',  action='store_true')
    parser.add_argument('-N', '--number', help='Download number wordlists',  action='store_true')
    parser.add_argument('-U', '--username', help='Download username wordlists',  action='store_true')
    parser.add_argument('-Ps', '--password', help='Download number wordlists',  action='store_true')
    parser.add_argument('-T', '--technology', help='Download technology wordlists',  action='store_true')
    parser.add_argument('-Py', '--payload', help='Download payload wordlists',  action='store_true')
    parser.add_argument('-Ws', '--webshell', help='Download webshell files',  action='store_true')
    parser.add_argument('-Dy', '--dynamic', help='Download/Update wordlists with dynamic tag',  action='store_true')
    parser.add_argument('-a', '--all', help='Download all wordlists',  action='store_true')
    parser.add_argument('-i', '--input', help='Input Json file contains objects of wordlist for download', type=pathlib.Path)
    parser.add_argument('-o', '--output', help='Output directory name for download wordlists [default = "./"]', default=None, type=str)
    parser.add_argument('-t', '--thread', help='Thread for download files [default = 4]', default=4, type=int)
    parser.add_argument('-s', '--silent', help='Silent mode',  action='store_true')
    parser.add_argument('-v', '--verbose', help='Verbose mode',  action='store_true')
    parser.add_argument('-r', '--retry', help='Retry to download wordlist [default = 3]', default=3, type=int)
    parser.add_argument('-to', '--timeout', help='Http Requet timeout [default = 5s]', default=5, type=int)
    parser.add_argument('-rd', '--retry-delay', help='Delay after each retry to downlaod [default = 2s]', default=2, type=int)
    
    args = parser.parse_args()
    subdomain = args.subdomain
    directory = args.directory
    api = args.api
    file = args.file
    word = args.word
    parameter = args.parameter
    header = args.header
    number = args.number
    username = args.username
    password = args.password
    technology = args.technology
    payload = args.payload
    webshell= args.webshell
    dynamic = args.dynamic
    all_w = args.all
    input_json = args.input
    output = args.output
    thread = args.thread
    silent = args.silent
    verbose = args.verbose
    retry = args.retry
    timeout = args.timeout
    retry_delay = args.retry_delay
    
    if len([arg for arg in sys.argv if arg in ['-a', '-all', '-i', '--input']]) > 1:
        parser.error(f"{Colors.RED}[-] Argument -a/--all|-i/--input: not allowed with more than one use!{Colors.RESET}")
    
    if sum(bool(arg) for arg in [all_w, input_json, any([subdomain, directory, api, file, word, parameter, header, number, username, password, technology, webshell, payload])]) != 1:
        parser.error(f"{Colors.RED}[-] You must provide exactly one of -a/--all, -i/--input or at least one of wordlist flag!{Colors.RESET}")
    
    
    if input_json:
        if not os.path.exists(input_json):
            parser.error(f"{Colors.RED}[-] The file {input_json} value of -i/--input flag does not exist!{Colors.RESET}")
        try:
            with open(input_json, 'r', encoding='utf-8') as file:
                json_loaded = json.load(file)
                wordlist_for_download = get_download_object(json_loaded, wordlist_flags, "input")
                
        except (json.JSONDecodeError, UnicodeDecodeError) as e:
            print(f"{Colors.RED}[!] Invalid JSON file: {e}{Colors.RESET}")
            sys.exit(1)

    elif all_w:
        wordlist_for_download = get_download_object(wordlist_objects, wordlist_flags, "all")
    
    elif any([subdomain, directory, api, file, word, parameter, header, number, username, password, technology, payload, webshell]):
        selected_wordlist = [arg for arg in wordlist_flags if getattr(args, arg)]
        wordlist_for_download = get_download_object(wordlist_objects, selected_wordlist, "custom")
        
    if dynamic:
        if verbose:
            print(f"{Colors.YELLOW}[+] Send Request to Assetnote for get update Links...{Colors.RESET}")
        assetnote_links = get_assetnote_link()
        wordlist_for_download = update_dynamic_obj(wordlist_for_download, assetnote_links)


    main_folder = os.path.join(output) if output else os.path.abspath(".")
    if not os.path.exists(main_folder):
        if verbose:
            print(f"{Colors.GREEN}[+] Create main directory: {main_folder} {Colors.RESET}")
        os.makedirs(main_folder, exist_ok=True)

    def make_wordlist_for_download(wordlist_obj):
        
        if all(wordlist_obj.get(key, False) for key in ( key1 for key1 in wordlist_obj.keys())):
            
            if not valid_url(wordlist_obj.get("url")):
                with print_lock:
                    print(f"{Colors.RED}{Colors.BOLD}[!]Not valid url! [{wordlist_obj.get("url")}]{Colors.RESET}")
                    if verbose:
                        print(f"{Colors.RED}{Colors.BOLD}[!] Check url of this object:\n{Colors.RESET}")
                        for key, value in wordlist_obj.items():
                            print(f"\t{Colors.YELLOW}{key} : {value}{Colors.RESET}")
                        print()
                return False
            
            folder = os.path.join(main_folder, wordlist_obj.get("folder"))
            if not os.path.exists(folder):
                os.makedirs(folder, exist_ok=True)
                if verbose:
                    print(f"\r{Colors.GREEN}[+] Create directory: {folder} {Colors.RESET}")
            
            download_file(wordlist_obj.get("url"), wordlist_obj.get("fileName"), wordlist_obj.get("folder"), os.path.join(folder, wordlist_obj.get("fileName")), retry, timeout, retry_delay, verbose, silent)
            
        else:
            with print_lock:
                print(f"\r{Colors.RED}{Colors.BOLD}[!] Missing something in object: {wordlist_obj.get("fileName", " No fileNmae! ")}{Colors.RESET}")
                if verbose:
                    print(f"\n\t{Colors.RED}==============={Colors.RESET}")
                    for key, value in wordlist_obj.items():
                        print(f"\t{Colors.GREEN}{key}{Colors.RESET}" if key else f"{Colors.RED}\t[Check!]{Colors.RESET}", f"{Colors.GREEN}{value}{Colors.RESET}" if value else f"{Colors.RED}[Check!]{Colors.RESET}", sep=" : ")
                    print(f"\t{Colors.RED}==============={Colors.RESET}")
                
    try:            
        with ThreadPoolExecutor(max_workers=thread) as executor:
            futures = [executor.submit(make_wordlist_for_download, wordlist_obj) for wordlist_obj in wordlist_for_download]
            for future in as_completed(futures):
                try:
                    future.result()
                except Exception as e:
                    if not silent:
                        print(f"{Colors.RED}\n[!] Exception in thread: {e}{Colors.RESET}")
                    
    except KeyboardInterrupt:
        print(f"\n{Colors.RED}[!] KeyboardInterrupt detected. Shutting down2...{Colors.RESET}")
        executor.shutdown(wait=False, cancel_futures=True)
        sys.exit(1)
        
    except Exception as e:
        if verbose:
            print(f"\n{Colors.RED}[!] Error in running ...\n {e}{Colors.RESET}")

if __name__ == "__main__":
    
    if not bool([arg for arg in sys.argv if arg in ['-s', '--silent']]):
        print(banner)
        
    main()